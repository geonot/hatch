{
  "name": "hatch-cli",
  "version": "0.0.1",
  "description": "A CLI tool for Hatch",
  "type": "module",
  "main": "bin/hatch.js",
  "bin": {
    "hatch": "bin/hatch.js"
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config '{\"testEnvironment\": \"jest-environment-jsdom\", \"transform\": {}, \"testMatch\": [\"**/tests/**/*.test.js\"]}'",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.json --coverage",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.json --watch",
    "lint": "echo 'Linting not configured yet'",
    "build": "echo 'Build completed - no compilation needed for ES modules'"
  },
  "keywords": [
    "cli",
    "hatch"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fs-extra": "^11.3.0",
    "js-yaml": "^4.1.0",
    "vite": "^6.3.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/sinon": "^17.0.3",
    "chai": "^5.2.0",
    "chai-as-promised": "^8.0.1",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.2",
    "sinon": "^18.0.0"
  }
}
