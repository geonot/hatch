{
  "preset": null,
  "transform": {},
  "testEnvironment": "node",
  "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
  "testMatch": [
    "<rootDir>/tests/**/*.test.js"
  ],
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)$": "$1"
  },
  "collectCoverageFrom": [
    "engine/**/*.js",
    "!engine/**/*.test.js",
    "!tests/**/*.js",
    "!**/node_modules/**"
  ],
  "verbose": true
}